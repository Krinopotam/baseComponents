{
  // These tasks will run in order when initializing your CodeSandbox project.
  "setupTasks": [
    {
      "name": "Install Dependencies",
      "command": "npm install"
    }
  ],

  // These tasks can be run from CodeSandbox. Running one will open a log in the app.
  "tasks": {
    "dev": {
      "name": "dev",
      "command": "npm run dev",
      "runAtStart": true
    },
    "prod": {
      "name": "prod",
      "command": "npm run prod",
      "runAtStart": false
    },
    "start": {
      "name": "start",
      "command": "npm start"
    },
    "ganerateFormConfigs": {
      "name": "ganerateFormConfigs",
      "command": "node ./resources/app/components/baseComponents/dForm/configGenerator/configGenerator.js true"
    },
    "ganerateExamples": {
      "name": "generateExamples",
      "command": "node ./resources/app/components/baseComponents/examples/examplesDescriptionGenerator.js"
    },
    "git config --global user.name \"Krinopotam\"": {
      "name": "git config --global user.name \"Krinopotam\"",
      "command": "git config --global user.name \"Krinopotam\""
    }
  }
}
